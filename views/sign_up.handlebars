<div class="uk-container-small my-container" id="signUpPage">
    <div class="container-inner-border">
        <div class="uk-flex uk-flex-center">
            <div class="uk-margin-medium-top loginCard">
                <img src="../assets/img/pokeTrader.png" alt="PokeTrader Logo" id="pokeLogo">
                <h3 class="uk-text-center">Your Trusted Source for Tracking your Card Collection Online</h3>
                <div class="uk-column" <div>
                    <form class="signup">
                        <div class="row">
                            <label for="firstName" class="uk-form-label">First Name</label>
                            <input type="text" class="uk-input" id="first-name">

                            <label for="lastName" class="uk-form-label">Last Name</label>
                            <input type="text" class="uk-input" id="last-name">
                        </div>
                        <div class="row">
                            <label for="email-address" class="uk-form-label">Email Address</label>
                            <input type="text" class="uk-input" id="email-input">

                            <label for="password" class="uk-form-label">Password</label>
                            <input type="password" class="uk-input" id="password-input">
                        </div>
                        <div class="uk-text-center">
                            <button type="submit" class="uk-button uk-button-default uk-margin-medium-top signUp-btn">Sign
                                Up</button>
                            <p>Already have an account?</p>
                            <a href="/" class="uk-link" id="loginLink">Log in</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <script type="text/javascript" src="../assets/js/sign_up.js"></script>